# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                       controllers.Application.index
GET     /dummy                                  controllers.Application.dummy
POST    /authenticate                           controllers.Application.login

GET     /signup                                 controllers.Application.signup
POST    /signup                                 controllers.Application.submit
GET     /fbOAuthReturn                          controllers.Application.fbOAuthReturn(state: String, code: String)

POST     /fbOAuth                               controllers.Application.fbOAuth

GET      /content/automobile/:id          controllers.Application.autoImage(id: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                   controllers.Assets.at(path="/public", file)

